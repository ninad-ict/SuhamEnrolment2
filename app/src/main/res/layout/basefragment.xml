<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/FrameBase"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textViewHeadingB"
            style="@style/TextAppearance.AppCompat.Title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_columnSpan="2"
            android:layout_marginBottom="60sp"
            android:background="@color/white"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="Enter Details for "
            android:textColor="@color/skyblue"
            android:textSize="20sp" />

        <GridLayout
            android:id="@+id/gridLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:columnCount="2"
            android:rowCount="6">



            <Button
                android:id="@+id/buttonReEnter"
                android:layout_width="140sp"
                android:layout_height="wrap_content"
                android:layout_row="5"
                android:layout_column="0"
                android:layout_gravity="center"
                android:layout_marginTop="20sp"
                android:background="@color/skyblue"
                android:onClick="reEnter"
                android:padding="4sp"
                android:text="Re-Enter Details"
                android:textColor="@color/white"

                />

            <Button
                android:id="@+id/buttonFinalSend"
                android:layout_width="140sp"
                android:layout_height="wrap_content"
                android:layout_row="5"
                android:layout_column="1"
                android:layout_gravity="center"
                android:layout_marginTop="20sp"
                android:layout_marginStart="10sp"
                android:background="@color/skyblue"
                android:onClick="finalSubmit"
                android:padding="4sp"
                android:text="Submit Details"
                android:textColor="@color/white" />

            <!--educationlogo3-->
            <ImageView
                android:id="@+id/imageViewEducation"
                android:layout_width="120sp"
                android:layout_height="120sp"
                android:layout_row="3"
                android:layout_column="0"
                android:layout_gravity="center"
                android:onClick="switchFragment"
                android:visibility="visible"
                app:srcCompat="@drawable/educationlogo3" />

            <ImageView
                android:id="@+id/imageViewChildBirth"
                android:layout_width="130sp"
                android:layout_height="130sp"
                android:layout_row="3"
                android:layout_column="0"
                android:layout_gravity="center"
                 android:onClick="switchFragment"
                android:visibility="invisible"
                app:srcCompat="@drawable/child1" />

           <!-- <ImageView
                android:id="@+id/imageViewPregnant"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:layout_row="3"
                android:layout_column="0"
                android:layout_gravity="center"
                android:layout_marginLeft="20sp"
                android:layout_marginRight="50sp"
                android:onClick="switchFragment"
                android:visibility="invisible"
                app:srcCompat="@drawable/pregnant7" />-->

            <ImageView
                android:id="@+id/imageViewPreNatal"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:layout_row="3"
                android:layout_column="0"
                android:layout_columnSpan="2"
                android:layout_gravity="center"
                android:layout_marginTop="50sp"
                android:onClick="switchFragment"
                android:visibility="invisible"
                app:srcCompat="@drawable/pregnant7" />


            <ImageView
                android:id="@+id/imageViewLocation"
                android:layout_width="120sp"
                android:layout_height="120sp"
                android:layout_row="1"
                android:layout_column="0"
                android:layout_gravity="center"
                android:contentDescription="TODO"
                android:onClick="switchFragment"
                app:srcCompat="@drawable/locationlogo2" />


            <ImageView
                android:id="@+id/imageViewPersonal"
                android:layout_width="120sp"
                android:layout_height="120sp"
                android:layout_row="1"
                android:layout_column="1"
                android:layout_gravity="center"
                android:onClick="switchFragment"
                android:padding="10sp"
                android:visibility="visible"
                app:srcCompat="@drawable/personallogo2" />

            <ImageView
                android:id="@+id/imageViewPersonalPreg"
                android:layout_width="120sp"
                android:layout_height="120sp"
                android:layout_row="1"
                android:layout_column="1"
                android:layout_gravity="center"
                android:onClick="switchFragment"
                android:visibility="invisible"
                app:srcCompat="@drawable/personallogo2" />

            <ImageView
                android:id="@+id/imageViewParents"
                android:layout_width="120sp"
                android:layout_height="120sp"
                android:layout_row="1"
                android:layout_column="1"
                android:layout_gravity="center"
                android:onClick="switchFragment"
                android:padding="10sp"
                android:visibility="invisible"
                app:srcCompat="@drawable/parents2" />

            <ImageView
                android:id="@+id/imageViewHealth"
                android:layout_width="120sp"
                android:layout_height="120sp"
                android:layout_row="3"
                android:layout_column="1"
                android:layout_gravity="center"
                android:onClick="switchFragment"
                android:visibility="visible"
                app:srcCompat="@drawable/healthlogo4" />

            <ImageView
                android:id="@+id/imageViewBabyDelivery"
                android:layout_width="120sp"
                android:layout_height="120sp"
                android:layout_row="3"
                android:layout_column="0"
                android:layout_columnSpan="2"
                android:layout_marginTop="30sp"
                android:layout_gravity="center"
                android:onClick="switchFragment"
                android:visibility="invisible"
                app:srcCompat="@drawable/babydelivery1" />

            <ImageView
                android:id="@+id/imageViewBabyGrowth"
                android:layout_width="120sp"
                android:layout_height="120sp"
                android:layout_row="3"
                android:layout_column="1"
                android:layout_gravity="center"
                android:onClick="switchFragment"
                android:padding="5sp"
                android:visibility="invisible"
                app:srcCompat="@drawable/babygrowth1" />


            <TextView
                android:id="@+id/textviewEducation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="4"
                android:layout_column="0"
                android:layout_gravity="center"
                android:fontFamily="sans-serif-black"
                android:text="Education"
                android:textSize="20dp"
                android:visibility="visible" />

            <TextView
                android:id="@+id/textviewChildBirth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="4"
                android:layout_column="0"
                android:layout_gravity="center"
                android:fontFamily="sans-serif-black"
                android:layout_marginRight="20sp"
                android:text="Child Birth"
                android:textSize="20dp"
                android:visibility="invisible" />

           <!-- <TextView
                android:id="@+id/textviewPregnant"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="4"
                android:layout_column="0"
                android:layout_gravity="center"
                android:layout_marginRight="20sp"
                android:fontFamily="sans-serif-black"
                android:text="Pregnancy"
                android:textSize="20dp"
                android:visibility="invisible" />-->

            <TextView
                android:id="@+id/textviewPreNatal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="4"
                android:layout_column="0"
                android:layout_columnSpan="2"
                android:layout_gravity="center"
                android:layout_marginTop="5sp"
                android:fontFamily="sans-serif-black"
                android:text="Pregnancy Details"
                android:textSize="20dp"
                android:visibility="invisible" />

            <TextView
                android:id="@+id/textviewLocation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="0"
                android:layout_gravity="center"
                android:fontFamily="sans-serif-black"
                android:text="Location"
                android:textSize="20dp" />


            <TextView
                android:id="@+id/textviewHealth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="4"
                android:layout_column="1"
                android:layout_gravity="center"
                android:fontFamily="sans-serif-black"
                android:text="Health"
                android:textSize="20dp"
                android:visibility="visible" />

            <TextView
                android:id="@+id/textviewBabyDelivery"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="4"
                android:layout_column="0"
                android:layout_columnSpan="2"
                android:layout_gravity="center"
                android:fontFamily="sans-serif-black"
                android:text="Delivery Details"
                android:textSize="20dp"
                android:visibility="invisible" />

            <TextView
                android:id="@+id/textviewBabyGrowth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="4"
                android:layout_column="1"
                android:layout_gravity="center"
                android:fontFamily="sans-serif-black"
                android:text="Child Growth"
                android:textSize="20dp"
                android:visibility="invisible" />


            <TextView
                android:id="@+id/textviewPersonal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="1"
                android:layout_gravity="center"
                android:fontFamily="sans-serif-black"
                android:text="Personal"
                android:textSize="20dp"
                android:visibility="visible" />

            <TextView
                android:id="@+id/textviewParents"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_column="1"
                android:layout_gravity="center"
                android:fontFamily="sans-serif-black"
                android:text="Parents"
                android:textSize="20dp"
                android:visibility="invisible" />
        </GridLayout>

    </LinearLayout>

    </ScrollView>

</FrameLayout>
